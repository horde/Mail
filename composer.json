{
    "name": "horde/mail",
    "description": "Mail library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Mail",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        }
    ],
    "version": "2.6.5",
