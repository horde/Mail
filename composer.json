    "suggest": {
        "pear-pear.horde.org/Horde_Smtp": "^1.7",
        "pear-pear.horde.org/Horde_Stream_Wrapper": "^2.1",
        "pear-pear.horde.org/Horde_Test": "^2.1",
        "pear-pear.php.net/Net_DNS2": "*",
